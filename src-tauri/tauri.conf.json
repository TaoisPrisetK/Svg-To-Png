{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Tool SVG-To-PNG",
  "version": "0.1.0",
  "identifier": "com.ks10.svg-to-png",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev -- --host 127.0.0.1 --port 5173",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "label": "main",
        "title": "SVG â†’ PNG",
        "width": 1240,
        "height": 830,
        "transparent": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}


